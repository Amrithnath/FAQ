{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
	"parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
			"experimentalObjectRestSpread": true
    }
  },
	"rules": {
		"no-console": 1,
		"indent": ["error", "tab"],
		"quotes": ["error", "single"],
		"semi": ["error", "never"]
	}
}
